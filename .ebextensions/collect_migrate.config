container_commands:
  00_docker_ps:
    command: docker ps -a
    leader_only: true
  01_printenv:
    command: printenv
    leader_only: true
  02_compress_collect_migrate:
    command: export CONTAINER=`docker ps -l -q -f 'status=running'` && "docker exec $CONTAINER /code/compress_collect_migrate.sh"
    leader_only: true
